{
    "name": "seferov/typhp",
    "description": "Enforce type declaring",
    "license": "MIT",
    "type": "library",
    "keywords": ["type hint", "strict typing"],
    "require": {
        "php": "^7.1",
        "nikic/php-parser": "^4.2",
        "phpdocumentor/reflection-docblock": "^4.3",
        "symfony/console": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/stopwatch": "^4.3",
        "symfony/process": "^4.3",
        "jean85/pretty-package-versions": "^1.2 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Seferov\\Typhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seferov\\Typhp\\Tests\\": "tests"
        }
    },
    "bin": ["bin/typhp"],
    "authors": [
        {
            "name": "Farhad Safarov",
            "email": "farhad.safarov@gmail.com"
        }
    ]
}
